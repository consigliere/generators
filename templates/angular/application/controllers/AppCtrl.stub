(function() {
    'use strict';

    angular.module('app.controllers').controller('AppCtrl', ['$scope', '$rootScope', function($scope, $rootScope) {
        $scope.$watch(function() {
            return $rootScope.title;
        }, function(newPage) {
            $scope.title = newPage || 'Application';
        });

    }]);
})();